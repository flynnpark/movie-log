{
  "name": "movie-log-backend",
  "version": "0.1.0",
  "description": "Movie.log",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc && yarn copy-graphql",
    "copy-graphql": "copy ./src/api/**/*.graphql ./.build/api/",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "predev": "yarn types",
    "dev:server": "nodemon --exec ts-node src/server.ts -e ts",
    "dev:serverless": "yarn build&& sls offline start --skipCacheInvalidation",
    "deploy:dev": "yarn build&& sls deploy",
    "deploy:prod": "yarn build&& sls deploy --stage prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wphestiraid/movie-log-backend.git"
  },
  "author": "Flynn <wphestiraid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wphestiraid/movie-log-backend/issues"
  },
  "homepage": "https://github.com/wphestiraid/movie-log-backend#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.36",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook-token": "^0.4.33",
    "@types/passport-jwt": "^3.0.1",
    "apollo-server-express": "^2.7.0",
    "bcrypt": "^3.0.6",
    "class-validator": "^0.9.1",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.1",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.6.2-bdaf070.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "serverless": "^1.48.3",
    "serverless-http": "^2.0.2",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.6.8",
    "babel-runtime": "^6.26.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.19.1",
    "serverless-offline": "^5.8.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}